{
  "presets": [
    [
      "@babel/env",
      { "modules": false }
    ],
    "@babel/react"
  ],
  "env": {
    "test": {
      "presets": [
        ["react-app"]
      ]
    }
  },
  "plugins": [
    "@babel/proposal-object-rest-spread",
    "@babel/proposal-optional-chaining",
    "@babel/syntax-dynamic-import",
    "@babel/proposal-class-properties",
    "babel-plugin-styled-components"
  ]
}
